{
  "name": "plume2",
  "version": "0.1.3",
  "description": "a lightweight of flux for mobile web",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "coverage": "jest --coverage",
    "compile": "./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc --watch",
    "types": "cp ./typings/index.d.ts ./dist",
    "build": "npm run compile && npm run types"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hufeng/plume.git"
  },
  "keywords": [
    "preact",
    "immutable",
    "flux",
    "mobile"
  ],
  "files": [
    "package.json",
    "dist",
    "README.md"
  ],
  "author": "hufeng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hufeng/plume/issues"
  },
  "homepage": "https://github.com/hufeng/plume#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "mitt": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^19.0.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-test-renderer": "15.4.2",
    "ts-jest": "^19.0.9",
    "typescript": "^2.2.2"
  }
}
